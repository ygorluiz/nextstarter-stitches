{
  "name": "nextstarter-stitches",
  "version": "0.1.0",
  "private": true,
  "author": "ygorluiz",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preinstall": "npx only-allow pnpm",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "clean": "find . -name node_modules -o -name .next -o -name .turbo -o -name dist -type d -prune | xargs rm -rf",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "pnpm upgrade-interactive",
    "up-latest": "pnpm up --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@ygorluiz/styles": "^1.2.15",
    "framer-motion": "^7.3.6",
    "next": "^12.3.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@types/react": "^18.0.21",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.22",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.3"
  }
}
